# jarvis-agent - Environment Variables
# Copy this file to .env and fill in your API keys

# Qwen (LLM/TTS/ASR)
QWEN_API_KEY=
QWEN_TTS_MODEL=

GLM_API_KEY=

OPENAI_BASE_URL=
OPENAI_API_KEY=
OPENAI_LLM_MODEL=


# WebSocket Transport Configuration
WEBSOCKET_HOST=0.0.0.0
